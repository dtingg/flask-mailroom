{% extends 'base.jinja2' %}

{% block subtitle %}Donor Report{% endblock subtitle %}

{% block content %}
<ul>

    {% if error %}<p>{{ error }}</p>{% endif %}
    {% for donation in donations %}
        <li><b>{{ donation.donor.name }}</b>: {{ donation.value }}</li>
    {% endfor %}
</ul>
{% endblock content %}
